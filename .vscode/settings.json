{
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "pdflatex âžž biber âžž pdflatex Ã— 2",
            "tools": [
                "pdflatex",
                "biber",
                "pdflatex",
